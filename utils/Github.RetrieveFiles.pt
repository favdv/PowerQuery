//This function can be used to get github functions into PowerBI
//Just copy it into a blank query
//In this case, for the "repo" variable a parameter can be used to get the github repo but can be a hardcoded string too, e.g. 
//https://api.github.com/repos/<owner>/<reponame>
// "extensions" allows you to filter on extensiona, e.g. in this repo the functions are ending with .pq and templates on.pt 
// "functionNameField" can be used to identify a string in the function from which the preserred function name can be derived.
// See also "Function.Template.pt" for more information. If not specified, the the path will be used and the forward slashes replaced with a full stop.
// "createFunctions" can be used to either convert the queries to functions (true) or simply create a record of functions, types and names (false).
//the "functionList" record in the "AddFunctionRecord" step might need updating on occasion. 
//The "List.GetSharedFunctions.pt" can be used to get the shared functions. 

(repo as text, extensions as list, functionNameField as text, optional createFunctions as nullable logical)=>
let
    createFunctions = if createFunctions = null then true else createFunctions,

    GetRepoInfo =  
        Table.SelectRows(
            Table.ExpandRecordColumn(
                Table.FromList(
                    Json.Document(
                        Web.Contents(repo
                        ,[RelativePath = "git/trees/main",Query=[recursive="1"]]
                        )
                    )[tree], 
                    Splitter.SplitByNothing(), null, null, ExtraValues.Error
                ), 
                "Column1", {"path", "url", "sha"}
            ), 
            each List.AnyTrue(List.Transform(extensions, (ext) => Text.EndsWith([path], ext)))
        ),

    GetRepoContent = 
        Table.AddColumn(
            Table.AddColumn(
                GetRepoInfo, "Content", each let sha = [sha] 
                in
                Text.FromBinary(Binary.FromText(Json.Document(
                    Web.Contents(
                        repo,[RelativePath ="git/blobs/"& sha ]
                    )
                )[content], 
                BinaryEncoding.Base64))
            ),
            "FunctionName", 
            each    if Text.Contains([Content], functionNameField&" =") = false and Text.Contains([Content], functionNameField&"=") = false 
                    then Text.Replace([path], "/",".") 
                    else Text.BetweenDelimiters(Text.BetweenDelimiters([Content], functionNameField, ","), """", """")
        ),

    
    AddFunctionRecord =if createFunctions = true then let 
        functionList = [
            AI.ExecuteInProc=AI.ExecuteInProc,
            AI.ExecuteInternal=AI.ExecuteInternal,
            AI.ExternalSort=AI.ExternalSort,
            AI.GetAutoMLEntity=AI.GetAutoMLEntity,
